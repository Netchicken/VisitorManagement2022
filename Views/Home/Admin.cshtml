

<div>
    <div class="row">
        <div class="col-3">
            <h4>Where</h4>
            <ul>
                @foreach (var item in (IEnumerable<Visitor>)ViewData["WhereQuery"])
                {
                    <li> @item.FirstName @item.LastName </li>
                }
            </ul>

            <h4>OrderBy ascendng visitors count</h4>
            <ul>
                @foreach (var item in (IEnumerable<StaffNames>)ViewData["OrderByQuery"])
                {
                    <li>   @item.Name  @item.VisitorCount    </li>
                }
            </ul>


        </div>
        <div class="col-3">


            <h4>Select</h4>
            <ul>
                @foreach (var item in (IEnumerable<StaffNames>)ViewData["SelectQuery"])
                {
                    <li>  @item.Name - @item.Department  </li>
                }
            </ul>


        </div>
        <div class="col-3">
            <h4>Group By Day</h4>
            <ul>
                @foreach (var item in (IEnumerable<VisitorManagement.ViewModels.GroupBy>)ViewData["GroupByQuery"])
                {
                    <li>  @item.Day  @item.Count visitors</li>
                }
            </ul>
        </div>
        <div class="col-3">
            <h4>Group By Staff</h4>
            <ul>
                @foreach (var item in (IEnumerable<VisitorManagement.ViewModels.GroupByStaff>)ViewData["GroupByStaffQuery"])
                {
                    <li>  @item.Staff  @item.Count visitors</li>
                }
            </ul>

        </div>



            @*<h4>Visitors in the last 2 days</h4>
            <ul>
                @foreach (var item in (IEnumerable<Visitor>)ViewData["Last7DaysVisitors"])
                {
                    <li>  @item.FirstName  @item.LastName  met with @item.StaffName.Name on @item.DateIn</li>
                }
            </ul>*@


        

    </div>
</div>
